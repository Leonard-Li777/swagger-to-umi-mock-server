  "eslint-plugin-prettier": "^3.1.0",
  "eslint-plugin-react": "7.11.1",
  "father": "^2.3.0",
  "form-data": "^2.3.3",
  "got": "9.3.2",
  "husky": "1.2.0",
  "lerna": "^3.6.0",
  "lerna-changelog": "0.8.2",
  "lint-staged": "8.1.0",
  "mkdirp": "^0.5.1",
  "moment": "^2.24.0",
